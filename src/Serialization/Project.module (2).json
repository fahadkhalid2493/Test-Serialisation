          {
            "path": "/Destination Datasource",
            "scope": "ItemAndDescendants",
            "allowedPushOperations": "createUpdateAndDelete"
          },
          {
            "path": "/Global Configuration",
            "scope": "ItemAndDescendants",
            "allowedPushOperations": "createUpdateAndDelete"
          },

          {
            "path": "/Global",
            "scope": "ItemAndDescendants",
            "allowedPushOperations": "createUpdateAndDelete"
          },


          {
            "path": "/gulfair-app",
            "scope": "ItemAndDescendants",
            "allowedPushOperations": "createUpdateAndDelete"
          },
          {
            "path": "*",
            "scope": "ignored"
          }
        ]
      }



