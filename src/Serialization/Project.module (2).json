            "allowedPushOperations": "createUpdateAndDelete"
          },

          {
            "path": "*",
            "scope": "ignored"
          }

        ]

      },

      {
        "name": "layout",
        "path": "/sitecore/layout/Layouts/Project",
        "rules": [
          {
            "path": "/gulfair-app",
            "scope": "ItemAndDescendants",
            "allowedPushOperations": "createUpdateAndDelete"
          },

          {
            "path": "*",
            "scope": "ignored"
          }

        ]
      },
      {
        "name": "system",
        "path": "/sitecore/system/Modules/DAM",
